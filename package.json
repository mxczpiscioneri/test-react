{
  "name": "react-spa",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon src/server/index.js --exec babel-node",
    "clean": "rm -rf dist && mkdir dist",
    "build:client": "webpack --progress -p && mv dist/assets/index.html dist && cp -R public/* dist",
    "build:server": "babel --ignore src/client -d ./dist ./src -s",
    "prepublish": "npm run clean",
    "publish": "npm run build:client && npm run build:server",
    "postinstall": "if [ $NPM_CONFIG_PRODUCTION ]; then npm run publish; fi",
    "start": "pm2 start dist -i max",
    "start-docker": "pm2-docker start server -i max",
    "deploy:stage": "heroku builds:create -a <APP_NAME>"
  },
  "author": "",
  "license": "MIT",
